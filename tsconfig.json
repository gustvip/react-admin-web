{
	"compilerOptions": {
		"allowJs": true,
		"module": "esnext",
		"target": "esnext",
		"lib": [
			"es6",
			"dom"
		],
		"strictNullChecks": true,
		"jsx": "preserve",
		"noImplicitAny": false,
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"moduleResolution": "node",
		"experimentalDecorators": true
	},
	"exclude": [
		"node_modules",
		"build",
		"public"
	]
}
