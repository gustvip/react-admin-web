{
	"env": {
		"development": {
			"presets": [
				[
					"env",
					{
						"targets": {
							"browsers": [
								">1%",
								"Firefox ESR",
								"ie >=10",
								"chrome >= 25",
								"firefox >= 30",
								"safari >= 7",
								"opera >= 15",
								"android >= 4.4",
								"ios >= 9"
							],
							"node": "8.11"
						},
						"modules": false,
						"useBuiltIns": true,
						"debug": true
					}
				],
				"react",
				"stage-0"
			],
			"plugins": [
				[
					"import",
					{
						"libraryName": "antd",
						"libraryDirectory": "es",
						"style": true
					}
				],
				[
					"transform-decorators-legacy",
					"transform-decorators"
				],
				[
					"transform-runtime",
					{
						"polyfill": false,
						"regenerator": true
					}
				]
			]
		},
		"test": {
			"presets": [
				[
					"env",
					{
						"modules": false
					}
				],
				"react",
				"stage-0"
			]
		}
	}
}
