{
	"name": "demo",
	"version": "0.0.1",
	"author": "joey <zhouhaifei1994@sina.com>",
	"description": "react+redux+react-router+axios+lodash+echarts+antd-design+typescript+webpack4.6",
	"main": "src/index",
	"scripts": {
		"dev": "node server.js",
		"build": "webpack --config ./build/webpack.config.prod.js",
		"check-code": "eslint ./src",
		"mock": "./node_modules/.bin/json-server --watch ./mock/db.json --routes ./mock/routes.json",
		"test": "BABEL_ENV=test jest --colors",
		"coverage": "BABEL_ENV=test jest --colors --coverage",
		"intl": "rimraf ./i18n-messages && BABEL_ENV=i18n babel --quiet ./src > /dev/null && babel-node --presets es2015 ./scripts/translateIntl.js && rimraf ./i18n-messages",
		"publish": "node ./scripts/publish.js /data/internal_verify_platform_web /static/internal_verify_platform_web/"
	},
	"dependencies": {
		"antd": "^3.4.4",
		"axios": "^0.18.0",
		"classnames": "^2.2.5",
		"echarts": "^4.0.4",
		"echarts-for-react": "^2.0.8",
		"es6-promise": "^4.2.4",
		"immutability-helper": "^2.5.0",
		"jquery": "^3.3.1",
		"js-cookie": "^2.2.0",
		"leaflet": "^1.3.1",
		"lodash": "^4.17.10",
		"moment": "^2.22.1",
		"onfire.js": "^1.0.6",
		"prop-types": "^15.6.0",
		"query-string": "6.0.0",
		"react": "^16.3.2",
		"react-dom": "^16.3.2",
		"react-intl": "^2.4.0",
		"react-redux": "^5.0.6",
		"react-router-dom": "^4.2.2",
		"redux": "3.7.2",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.2.0",
		"url-search-params-polyfill": "^3.0.0"
	},
	"devDependencies": {
		"autoprefixer": "^8.4.1",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.0.2",
		"babel-jest": "^22.4.3",
		"babel-loader": "^7.1.2",
		"babel-plugin-import": "^1.6.2",
		"babel-plugin-react-intl": "^2.3.1",
		"babel-plugin-transform-decorators": "^6.24.1",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"bundle-loader": "^0.5.5",
		"clean-webpack-plugin": "^0.1.19",
		"css-loader": "^0.28.7",
		"css-to-string-loader": "^0.1.3",
		"enzyme": "^3.2.0",
		"eslint": "^4.11.0",
		"eslint-config-alloy": "^1.4.2",
		"file-loader": "^1.1.5",
		"jest": "^22.4.3",
		"less": "2.7.3",
		"less-loader": "^4.0.5",
		"mini-css-extract-plugin": "^0.4.0",
		"node-sass": "^4.9.0",
		"optimize-css-assets-webpack-plugin": "^4.0.0",
		"postcss-flexbugs-fixes": "^3.3.1",
		"postcss-loader": "^2.0.8",
		"react-test-renderer": "^16.1.1",
		"sass-loader": "^7.0.1",
		"ts-loader": "^4.2.0",
		"typescript": "^2.8.3",
		"uglifyjs-webpack-plugin": "^1.2.5",
		"url-loader": "^1.0.1",
		"webpack": "^4.6.0",
		"webpack-bundle-analyzer": "^2.11.1",
		"webpack-cli": "^2.0.15",
		"webpack-dev-server": "^3.1.3",
		"webpack-merge": "^4.1.2"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"jsx"
		],
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
			".*\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
		},
		"transform": {
			"^.+\\.js(x)?$": "babel-jest"
		},
		"setupFiles": [
			"<rootDir>/__mocks__/setup.js"
		]
	},
	"license": "ISC"
}
